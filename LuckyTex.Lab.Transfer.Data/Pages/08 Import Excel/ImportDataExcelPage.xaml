<UserControl x:Class="LuckyTex.Pages.ImportDataExcelPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="845" 
             d:DesignWidth="1200" Loaded="UserControl_Loaded" Unloaded="UserControl_Unloaded">

    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/LuckyTex.AirBag.Controls;component/Styles/ButtonStyles.xaml" />
                <ResourceDictionary Source="pack://application:,,,/LuckyTex.AirBag.Controls;component/Styles/GroupBoxStyle.xaml" />
                <ResourceDictionary Source="pack://application:,,,/LuckyTex.AirBag.Controls;component/Styles/RadioButtonStyle.xaml" />
                <ResourceDictionary Source="pack://application:,,,/LuckyTex.AirBag.Controls;component/Styles/CheckedBoxStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <Style x:Key="CenterCellStyle" TargetType="DataGridCell">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="DataGridCell">
                            <Grid Background="{TemplateBinding Background}">
                                <ContentPresenter HorizontalAlignment="Center"
                                      VerticalAlignment="Center"/>
                            </Grid>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>

            <Style x:Key="RightCellStyle" TargetType="DataGridCell">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="DataGridCell">
                            <Grid Background="{TemplateBinding Background}">
                                <ContentPresenter HorizontalAlignment="Right"
                                      VerticalAlignment="Center"/>
                            </Grid>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
        </ResourceDictionary>
    </UserControl.Resources>

    <DockPanel LastChildFill="True">
        <!-- Caption Bar -->
        <Border DockPanel.Dock="Top" CornerRadius="3" Margin="3, 3, 3, 0"
                Background="CornflowerBlue"
                Height="40">
            <TextBlock Text="LAB LOAD Excel"
                       Foreground="White"
                       FontSize="30" 
                       FontWeight="Bold"
                       TextAlignment="Center"
                       VerticalAlignment="Center"/>
        </Border>
        <!-- Button Command Bars -->
        <DockPanel DockPanel.Dock="Bottom" LastChildFill="True" 
                   Background="#F0F0F0"
                   Margin="5, 0">
            <!-- Back -->
            <StackPanel DockPanel.Dock="Right" Orientation="Horizontal">
                <Button x:Name="cmdBack"
                        Content="Back"
                        Margin="2" 
                        Width="150"
                        FontSize="30" 
                        FontWeight="Bold"
                        Style="{StaticResource MenuButtonStyle}" 
                        Click="cmdBack_Click" />
            </StackPanel>
            <Grid/>
        </DockPanel>
        <!-- Content -->
        <Border CornerRadius="3" Margin="3" 
                BorderThickness="2" BorderBrush="Black">
            <ScrollViewer HorizontalContentAlignment="Center"
                          VerticalContentAlignment="Center"
                          HorizontalScrollBarVisibility="Auto"
                          VerticalScrollBarVisibility="Auto">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="1185*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="5" />
                        <RowDefinition Height="45" />
                        <RowDefinition Height="5" />
                        <RowDefinition Height="500*" />
                        <RowDefinition Height="45" />
                        <RowDefinition Height="146" />
                        <RowDefinition Height="45" />
                        <RowDefinition Height="5" />
                    </Grid.RowDefinitions>
                    <Button Name="cmdSave" 
                            Content="Save Data"
                            Margin="3,3,0,0" 
                            FontSize="22"
                            FontWeight="Bold"
                            Height="40" Width="120" HorizontalAlignment="Left"  VerticalAlignment="Top" Style="{StaticResource MenuButtonStyle}" Grid.Row="1" Grid.Column="1" Click="cmdSave_Click">
                    </Button>
                    <Button Name="cmdSave2" 
                            Content="Save 2"
                            Margin="130,3,0,0" 
                            FontSize="22"
                            FontWeight="Bold"
                            Height="40" Width="120" HorizontalAlignment="Left"  VerticalAlignment="Top" Style="{StaticResource MenuButtonStyle}" Grid.Row="1" Grid.Column="1" Click="cmdSave2_Click">
                    </Button>
                    <ScrollViewer HorizontalContentAlignment="Center"
                          VerticalContentAlignment="Center"
                          HorizontalScrollBarVisibility="Auto"
                          VerticalScrollBarVisibility="Auto" Margin="3" Grid.Row="3" Grid.Column="1">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto"/>
                                <RowDefinition Height="546*"/>
                            </Grid.RowDefinitions>
                            <ListView x:Name="listView">
                                <ListView.View>
                                    <GridView>            
                                        <GridViewColumn Header="FILE NAME" DisplayMemberBinding="{Binding FILENAME}" Width="450"/>
                                        <GridViewColumn Header="Sum Row" DisplayMemberBinding="{Binding SUMROW}" Width="80"/>
                                        <GridViewColumn Header="Sum Err" DisplayMemberBinding="{Binding SUMERR}" Width="80"/>
                                        <GridViewColumn Header="ERROR" DisplayMemberBinding="{Binding ERROR}" Width="450"/>
                                    </GridView>
                                </ListView.View>
                            </ListView>
                            <!--<ListView x:Name="listView">
                                <ListView.View>
                                    <GridView>
                                        <GridViewColumn Header="NO" DisplayMemberBinding="{Binding NO}" Width="80"/>
                                        <GridViewColumn Header="ITEM CODE" DisplayMemberBinding="{Binding ITMCODE}" Width="180"/>
                                        <GridViewColumn Header="WEAVINGLOG" DisplayMemberBinding="{Binding WEAVINGLOG}" Width="180"/>
                                        <GridViewColumn Header="FINISHINGLOT" DisplayMemberBinding="{Binding FINISHINGLOT}" Width="180"/>

                                        <GridViewColumn Header="STATUS" DisplayMemberBinding="{Binding STATUS}" Width="100"/>
                                        <GridViewColumn Header="ERROR" DisplayMemberBinding="{Binding ERROR}" Width="250"/>
                                    </GridView>
                                </ListView.View>
                            </ListView>-->
                        </Grid>
                    </ScrollViewer>

                    <Button Name="cmdSearch" 
                            Content="Test Load Excel"
                            Margin="3,3,0,0" 
                            FontSize="22"
                            FontWeight="Bold"
                            Height="40" Width="180" HorizontalAlignment="Left"  VerticalAlignment="Top" Style="{StaticResource MenuButtonStyle}" Grid.Row="4" Grid.Column="1" Click="cmdSearch_Click">

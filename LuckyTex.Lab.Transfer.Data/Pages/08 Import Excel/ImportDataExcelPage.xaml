                            FontWeight="Bold"
                            Height="40" Width="107" HorizontalAlignment="Left"  VerticalAlignment="Top" Style="{StaticResource MenuButtonStyle}" Grid.Row="4" Grid.Column="1" Click="cmdClear_Click">

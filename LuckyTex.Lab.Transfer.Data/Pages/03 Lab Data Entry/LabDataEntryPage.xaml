                                        <TextBox FontSize="20" FontWeight="Bold" Margin="3,7,3,0" Name="txtELONGATIONFORCE_WAve" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="3" Grid.Column="9" PreviewTextInput="Cal_PreviewTextInput" IsReadOnly="True"/>


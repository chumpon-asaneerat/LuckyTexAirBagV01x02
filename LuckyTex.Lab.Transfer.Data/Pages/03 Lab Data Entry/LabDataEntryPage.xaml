                                        <TextBox FontSize="20" FontWeight="Bold" Margin="3,7,3,0" Name="txtELONGATIONFORCE_WN5" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="3" Grid.Column="7" PreviewTextInput="Cal_PreviewTextInput" LostFocus="ELONGATIONFORCE_W_LostFocus" KeyDown="txtELONGATIONFORCE_WN5_KeyDown" />

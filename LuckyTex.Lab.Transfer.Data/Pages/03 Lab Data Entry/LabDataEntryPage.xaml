                                        <TextBox FontSize="20" FontWeight="Bold" Margin="3,7,3,0" Name="txtELONGATIONFORCE_WYarn" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="3" Grid.Column="1" Text="Warp" IsReadOnly="True"/>
                                        <TextBox FontSize="20" FontWeight="Bold" Margin="3,7,3,0" Name="txtELONGATIONFORCE_WSpecification" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="3" Grid.Column="2" IsReadOnly="True" />

                                        <TextBox FontSize="20" FontWeight="Bold" Margin="3,7,3,0" Name="txtMAXFORCE_WAve" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="1" Grid.Column="9" PreviewTextInput="Cal_PreviewTextInput" IsReadOnly="True"/>


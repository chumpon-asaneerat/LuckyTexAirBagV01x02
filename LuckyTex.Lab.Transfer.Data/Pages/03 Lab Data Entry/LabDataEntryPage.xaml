                                        <TextBox FontSize="20" FontWeight="Bold" Margin="3,7,3,0" Name="txtELONGATIONFORCE_WN2" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="3" Grid.Column="4" PreviewTextInput="Cal_PreviewTextInput" LostFocus="ELONGATIONFORCE_W_LostFocus" KeyDown="txtELONGATIONFORCE_WN2_KeyDown" />

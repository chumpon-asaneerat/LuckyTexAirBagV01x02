<UserControl x:Class="LuckyTex.Pages.LabDataEntryPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="1465" 
             d:DesignWidth="1300" Loaded="UserControl_Loaded" Unloaded="UserControl_Unloaded">
    
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/LuckyTex.AirBag.Controls;component/Styles/ButtonStyles.xaml" />
                <ResourceDictionary Source="pack://application:,,,/LuckyTex.AirBag.Controls;component/Styles/GroupBoxStyle.xaml" />
                <ResourceDictionary Source="pack://application:,,,/LuckyTex.AirBag.Controls;component/Styles/RadioButtonStyle.xaml" />
                <ResourceDictionary Source="pack://application:,,,/LuckyTex.AirBag.Controls;component/Styles/CheckedBoxStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <Style x:Key="CenterCellStyle" TargetType="DataGridCell">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="DataGridCell">
                            <Grid Background="{TemplateBinding Background}">
                                <ContentPresenter HorizontalAlignment="Center"
                                      VerticalAlignment="Center"/>
                            </Grid>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>

            <Style x:Key="RightCellStyle" TargetType="DataGridCell">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="DataGridCell">
                            <Grid Background="{TemplateBinding Background}">
                                <ContentPresenter HorizontalAlignment="Right"
                                      VerticalAlignment="Center"/>
                            </Grid>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
        </ResourceDictionary>
    </UserControl.Resources>

    <DockPanel LastChildFill="True">
        <!-- Caption Bar -->
        <Border DockPanel.Dock="Top" CornerRadius="3" Margin="3, 3, 3, 0"
                Background="CornflowerBlue"
                Height="40">
            <TextBlock Text="Lab Data Entry Menu"
                       Foreground="White"
                       FontSize="30" 
                       FontWeight="Bold"
                       TextAlignment="Center"
                       VerticalAlignment="Center"/>
        </Border>
        <!-- Button Command Bars -->
        <DockPanel DockPanel.Dock="Bottom" LastChildFill="True" 
                   Background="#F0F0F0"
                   Margin="5, 0">
            <!-- Back -->
            <StackPanel DockPanel.Dock="Right" Orientation="Horizontal">
                <Button x:Name="cmdBack"
                        Content="Back"
                        Margin="2" 
                        Width="150"
                        FontSize="30" 
                        FontWeight="Bold"
                        Style="{StaticResource MenuButtonStyle}" 
                        Click="cmdBack_Click" />
            </StackPanel>
            <Grid/>
        </DockPanel>
        <!-- Content -->
        <Border CornerRadius="3" Margin="3" 
                BorderThickness="2" BorderBrush="Black">
            <ScrollViewer HorizontalContentAlignment="Center"
                          VerticalContentAlignment="Center"
                          HorizontalScrollBarVisibility="Auto"
                          VerticalScrollBarVisibility="Auto">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1" />
                        <ColumnDefinition Width="1185*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="5" />
                        <RowDefinition Height="45" />
                        <RowDefinition Height="5" />
                        <RowDefinition Height="555*" />
                        <RowDefinition Height="45" />
                        <RowDefinition Height="45" />
                        <RowDefinition Height="5" />
                    </Grid.RowDefinitions>
                    <Label Content="Item Code : " Grid.Column="1" Grid.Row="1" Margin="1,4,0,0" Name="labITMCODE" 
                           FontSize="20" FontWeight="Bold" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                    <TextBox x:Name="txtITMCODE"  
                                    Margin="126,8,0,0" Grid.Row="1"
                                    CharacterCasing="Upper"
                                    FontSize="20"
                                    FontWeight="Bold"
                                    TextAlignment="Center" VerticalAlignment="Top" Grid.Column="1" Width="150" HorizontalAlignment="Left" KeyDown="txtITMCODE_KeyDown" LostFocus="txtITMCODE_LostFocus">
                    </TextBox>
                    <Label Name="labWEAVINGLOG" Content="Production Lot : " FontSize="20" FontWeight="Bold" Grid.Column="1" Grid.Row="1" HorizontalAlignment="Left" Margin="279,4,0,0" VerticalAlignment="Top" />
                    <TextBox Name="txtWEAVINGLOG" CharacterCasing="Upper" FontSize="20" FontWeight="Bold" Grid.Column="1" Grid.Row="1" HorizontalAlignment="Left" Margin="447,8,0,0" TextAlignment="Center" VerticalAlignment="Top" Width="150" KeyDown="txtWEAVINGLOG_KeyDown" LostFocus="txtWEAVINGLOG_LostFocus" />
                    <Label Name="labFINISHINGLOT" Content="Finishing Lot : " FontSize="20" FontWeight="Bold" Grid.Column="1" Grid.Row="1" HorizontalAlignment="Left" Margin="600,4,0,0" VerticalAlignment="Top" />
                    <TextBox Name="txtFINISHINGLOT" CharacterCasing="Upper" FontSize="20" FontWeight="Bold" Grid.Column="1" Grid.Row="1" HorizontalAlignment="Left" Margin="749,8,0,0" TextAlignment="Center" VerticalAlignment="Top" Width="150" KeyDown="txtFINISHINGLOT_KeyDown" LostFocus="txtFINISHINGLOT_LostFocus" />
                    <Label Content="Entry Date : " FontSize="20" FontWeight="Bold" Grid.Column="1" Grid.Row="1" HorizontalAlignment="Left" Margin="902,4,0,0" Name="labEntryDate" VerticalAlignment="Top" />
                    <DatePicker Name="dteEntryDate" 
                                 Margin="1029,3,0,0"
                                 FontSize="24" 
                                 FontWeight="Bold" 
                                 Foreground="Black" 
                                 VerticalAlignment="Top" 
                                 Grid.Column="1" Grid.Row="1" Width="160" HorizontalAlignment="Left" />
                    <Button Name="cmdClear" 
                            Content="Clear"
                            Margin="3,1,0,0" 
                            FontSize="22"
                            FontWeight="Bold"
                            Height="40" Width="107" HorizontalAlignment="Left"  VerticalAlignment="Top" Style="{StaticResource MenuButtonStyle}" Grid.Row="4" Grid.Column="1" Click="cmdClear_Click">
                    </Button>
                    <Grid Margin="3" Grid.Row="3" Grid.Column="1" >
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="385*"/>
                                <RowDefinition Height="275*" />
                            </Grid.RowDefinitions>
                            <Border BorderBrush="Black" BorderThickness="2" Grid.Row="1" Margin="1">
                                <Grid>
                                    <Grid Grid.Row="1" Margin="89,0,0,0" Name="grid1">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="45" />
                                            <RowDefinition Height="42" />
                                            <RowDefinition Height="42" />
                                            <RowDefinition Height="42" />
                                            <RowDefinition Height="42" />
                                            <RowDefinition Height="42" />
                                            <RowDefinition Height="42" />
                                            <RowDefinition Height="42" />
                                            <RowDefinition Height="42" />
                                            <RowDefinition Height="4*" />
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="180" />
                                            <ColumnDefinition Width="80" />
                                            <ColumnDefinition Width="140" />
                                            <ColumnDefinition Width="100" />
                                            <ColumnDefinition Width="100" />
                                            <ColumnDefinition Width="100" />
                                            <ColumnDefinition Width="100" />
                                            <ColumnDefinition Width="100" />
                                            <ColumnDefinition Width="100" />
                                            <ColumnDefinition Width="100" />
                                            <ColumnDefinition Width="100" />
                                            <ColumnDefinition Width="4*" />
                                        </Grid.ColumnDefinitions>
                                        <Label Content="Method" FontSize="20" FontWeight="Bold" Margin="33,3,33,0" Name="labMethodPDF" VerticalAlignment="Top" HorizontalAlignment="Center"/>
                                        <Label Content="Yarn" FontSize="20" FontWeight="Bold" HorizontalAlignment="Center" Margin="0,3,0,0" Name="labYarnPDF" VerticalAlignment="Top" Grid.Column="1" />
                                        <Label Content="Specification" FontSize="20" FontWeight="Bold" HorizontalAlignment="Center" Margin="0,3,0,0"  Name="labSpecificationPDF" VerticalAlignment="Top" Grid.Column="2" />
                                        <Label Content="N1" FontSize="20" FontWeight="Bold" HorizontalAlignment="Center" Margin="0,3,0,0" Name="labN1PDF" VerticalAlignment="Top" Grid.Column="3" />
                                        <Label Content="N2" FontSize="20" FontWeight="Bold" HorizontalAlignment="Center" Margin="0,3,0,0" Name="labN2PDF" VerticalAlignment="Top" Grid.Column="4" />
                                        <Label Content="N3" FontSize="20" FontWeight="Bold" HorizontalAlignment="Center" Margin="0,3,0,0" Name="labN3PDF" VerticalAlignment="Top" Grid.Column="5" />
                                        <Label Content="N4" FontSize="20" FontWeight="Bold" HorizontalAlignment="Center" Margin="0,3,0,0" Name="labN4PDF" VerticalAlignment="Top" Grid.Column="6" />
                                        <Label Content="N5" FontSize="20" FontWeight="Bold" HorizontalAlignment="Center" Margin="0,3,0,0" Name="labN5PDF" VerticalAlignment="Top" Grid.Column="7" />
                                        <Label Content="N6" FontSize="20" FontWeight="Bold" HorizontalAlignment="Center" Margin="0,3,0,0" Name="labN6PDF" VerticalAlignment="Top" Grid.Column="8" />

                                        <TextBox FontSize="20" FontWeight="Bold" Margin="3,7,3,0" Name="txtMAXFORCE_FN5" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="2" Grid.Column="7" PreviewTextInput="Cal_PreviewTextInput" LostFocus="MAXFORCE_F_LostFocus" KeyDown="txtMAXFORCE_FN5_KeyDown" />
                                        <TextBox FontSize="20" FontWeight="Bold" Margin="3,7,3,0" Name="txtMAXFORCE_FN6" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="2" Grid.Column="8" PreviewTextInput="Cal_PreviewTextInput" LostFocus="MAXFORCE_F_LostFocus" KeyDown="txtMAXFORCE_FN6_KeyDown" />

                                        <TextBox FontSize="20" FontWeight="Bold" Margin="3,7,3,0" Name="txtMAXFORCE_FAve" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="2" Grid.Column="9" PreviewTextInput="Cal_PreviewTextInput" IsReadOnly="True"/>

                                        <!-- ELONGATIONFORCE_W -->

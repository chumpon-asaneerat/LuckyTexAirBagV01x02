<Window x:Class="LuckyTex.Windows.LabDataEntryWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Height="765" 
             Width="1200"  
        ShowInTaskbar="False"
        Topmost="True"
        WindowStartupLocation="CenterScreen"
        MinHeight="300" MinWidth="500" Loaded="UserControl_Loaded" Unloaded="UserControl_Unloaded">

    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/LuckyTex.AirBag.Controls;component/Styles/ButtonStyles.xaml" />
                <ResourceDictionary Source="pack://application:,,,/LuckyTex.AirBag.Controls;component/Styles/GroupBoxStyle.xaml" />
                <ResourceDictionary Source="pack://application:,,,/LuckyTex.AirBag.Controls;component/Styles/RadioButtonStyle.xaml" />
                <ResourceDictionary Source="pack://application:,,,/LuckyTex.AirBag.Controls;component/Styles/CheckedBoxStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <Style x:Key="CenterCellStyle" TargetType="DataGridCell">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="DataGridCell">
                            <Grid Background="{TemplateBinding Background}">
                                <ContentPresenter HorizontalAlignment="Center"
                                      VerticalAlignment="Center"/>
                            </Grid>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>

            <Style x:Key="RightCellStyle" TargetType="DataGridCell">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="DataGridCell">
                            <Grid Background="{TemplateBinding Background}">
                                <ContentPresenter HorizontalAlignment="Right"
                                      VerticalAlignment="Center"/>
                            </Grid>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
        </ResourceDictionary>
    </Window.Resources>

    <DockPanel LastChildFill="True">
        <!-- Caption Bar -->
        <Border DockPanel.Dock="Top" CornerRadius="3" Margin="3, 3, 3, 0"
                Background="CornflowerBlue"
                Height="40">
            <TextBlock Text="Lab Result Detail"
                       Foreground="White"
                       FontSize="30" 
                       FontWeight="Bold"
                       TextAlignment="Center"
                       VerticalAlignment="Center"/>
        </Border>
        <!-- Button Command Bars -->
        <DockPanel DockPanel.Dock="Bottom" LastChildFill="True" 
                   Background="#F0F0F0"
                   Margin="5, 0">
            <!-- Back -->
            <StackPanel DockPanel.Dock="Right" Orientation="Horizontal">
                <Grid>
                <Button Name="cmdApprove" Content="Approve" Margin="0,0,331,1" FontSize="22" FontWeight="Bold" Height="40" Style="{StaticResource MenuButtonStyle}" Width="165" Click="cmdApprove_Click" HorizontalAlignment="Right" />
                <Button Name="cmdNotApprove" Content="Not Approve" Margin="0,0,159,1" FontSize="22" FontWeight="Bold" Height="40" Width="165" Style="{StaticResource MenuButtonStyle}" Click="cmdNotApprove_Click" HorizontalAlignment="Right"></Button>
                <Button x:Name="cmdBack"
                        Content="Close"
                        Margin="0,2,2,2" 
                        Width="150"
                        FontSize="30" 
                        FontWeight="Bold"
                        Style="{StaticResource MenuButtonStyle}" 
                        Click="cmdBack_Click" HorizontalAlignment="Right" />
                </Grid>
            </StackPanel>
            <Grid>
                <TextBlock Text="Status : " Margin="10,10,0,0" FontSize="18" FontWeight="Bold" TextAlignment="Center" HorizontalAlignment="Left" VerticalAlignment="Top" />
                <TextBox x:Name="txtStatus" Margin="84,9,0,0" IsReadOnly="True" CharacterCasing="Upper" FontSize="18" FontWeight="Bold" TextAlignment="Center" VerticalAlignment="Top" HorizontalAlignment="Left" Width="212"></TextBox>
            </Grid>
        </DockPanel>
        <!-- Content -->
        <Border CornerRadius="3" Margin="3" 
                BorderThickness="2" BorderBrush="Black">
            <ScrollViewer HorizontalContentAlignment="Center"
                          VerticalContentAlignment="Center"
                          HorizontalScrollBarVisibility="Auto"
                          VerticalScrollBarVisibility="Auto">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1" />
                        <ColumnDefinition Width="1185*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="5" />
                        <RowDefinition Height="45" />
                        <RowDefinition Height="5" />
                        <RowDefinition Height="645*" />
                        <RowDefinition Height="5" />
                    </Grid.RowDefinitions>
                    <Label Content="Item Code : " Grid.Column="1" Grid.Row="1" Margin="2,4,0,0" Name="labITMCODE" 
                           FontSize="18" FontWeight="Bold" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                    <TextBox x:Name="txtITMCODE"  
                                    Margin="117,8,0,0" Grid.Row="1"
                                    CharacterCasing="Upper"
                                    FontSize="18"
                                    FontWeight="Bold"
                                    TextAlignment="Center" VerticalAlignment="Top" Grid.Column="1" Width="150" HorizontalAlignment="Left" KeyDown="txtITMCODE_KeyDown" >
                    </TextBox>
                    <Label Name="labWEAVINGLOG" Content="Production Lot : " FontSize="18" FontWeight="Bold" Grid.Column="1" Grid.Row="1" HorizontalAlignment="Left" Margin="269,4,0,0" VerticalAlignment="Top" />
                    <TextBox Name="txtWEAVINGLOG" CharacterCasing="Upper" FontSize="18" FontWeight="Bold" Grid.Column="1" Grid.Row="1" HorizontalAlignment="Left" Margin="423,8,0,0" TextAlignment="Center" VerticalAlignment="Top" Width="150" KeyDown="txtWEAVINGLOG_KeyDown" />
                    <Label Name="labFINISHINGLOT" Content="Finishing Lot : " FontSize="18" FontWeight="Bold" Grid.Column="1" Grid.Row="1" HorizontalAlignment="Left" Margin="576,4,0,0" VerticalAlignment="Top" />
                    <TextBox Name="txtFINISHINGLOT" CharacterCasing="Upper" FontSize="18" FontWeight="Bold" Grid.Column="1" Grid.Row="1" HorizontalAlignment="Left" Margin="712,8,0,0" TextAlignment="Center" VerticalAlignment="Top" Width="150" KeyDown="txtFINISHINGLOT_KeyDown"/>
                    <Label Content="Entry Date : " FontSize="18" FontWeight="Bold" Grid.Column="1" Grid.Row="1" HorizontalAlignment="Left" Margin="865,4,0,0" Name="labEntryDate" VerticalAlignment="Top" />
                    <DatePicker Name="dteEntryDate" 
                                 Margin="984,5,0,0"
                                 FontSize="18" 
                                 FontWeight="Bold" 
                                 Foreground="Black" 
                                 VerticalAlignment="Top" 
                                 Grid.Column="1" Grid.Row="1" Width="160" HorizontalAlignment="Left" />
                    <Grid Margin="3" Grid.Row="3" Grid.Column="1" >
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="385*"/>
                                <RowDefinition Height="275*" />
                            </Grid.RowDefinitions>
                            <Border BorderBrush="Black" BorderThickness="2" Grid.Row="1" Margin="1">
                                <Grid>
                                    <Grid Grid.Row="1" Margin="32,0,0,0" Name="grid1">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="45" />
                                            <RowDefinition Height="42" />
                                            <RowDefinition Height="42" />
                                            <RowDefinition Height="42" />
                                            <RowDefinition Height="42" />
                                            <RowDefinition Height="42" />
                                            <RowDefinition Height="42" />
                                            <RowDefinition Height="42" />
                                            <RowDefinition Height="42" />
                                            <RowDefinition Height="4*" />
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="180" />
                                            <ColumnDefinition Width="80" />
                                            <ColumnDefinition Width="140" />
                                            <ColumnDefinition Width="100" />
                                            <ColumnDefinition Width="100" />
                                            <ColumnDefinition Width="100" />
                                            <ColumnDefinition Width="100" />
                                            <ColumnDefinition Width="100" />
                                            <ColumnDefinition Width="100" />
                                            <ColumnDefinition Width="100" />
                                            <ColumnDefinition Width="4*" />
                                        </Grid.ColumnDefinitions>
                                        <Label Content="Method" FontSize="18" FontWeight="Bold" Margin="33,3,33,0" Name="labMethodPDF" VerticalAlignment="Top" HorizontalAlignment="Center"/>
                                        <Label Content="Yarn" FontSize="18" FontWeight="Bold" HorizontalAlignment="Center" Margin="0,3,0,0" Name="labYarnPDF" VerticalAlignment="Top" Grid.Column="1" />
                                        <Label Content="Specification" FontSize="18" FontWeight="Bold" HorizontalAlignment="Center" Margin="0,3,0,0"  Name="labSpecificationPDF" VerticalAlignment="Top" Grid.Column="2" />
                                        <Label Content="N1" FontSize="18" FontWeight="Bold" HorizontalAlignment="Center" Margin="0,3,0,0" Name="labN1PDF" VerticalAlignment="Top" Grid.Column="3" />
                                        <Label Content="N2" FontSize="18" FontWeight="Bold" HorizontalAlignment="Center" Margin="0,3,0,0" Name="labN2PDF" VerticalAlignment="Top" Grid.Column="4" />
                                        <Label Content="N3" FontSize="18" FontWeight="Bold" HorizontalAlignment="Center" Margin="0,3,0,0" Name="labN3PDF" VerticalAlignment="Top" Grid.Column="5" />
                                        <Label Content="N4" FontSize="18" FontWeight="Bold" HorizontalAlignment="Center" Margin="0,3,0,0" Name="labN4PDF" VerticalAlignment="Top" Grid.Column="6" />
                                        <Label Content="N5" FontSize="18" FontWeight="Bold" HorizontalAlignment="Center" Margin="0,3,0,0" Name="labN5PDF" VerticalAlignment="Top" Grid.Column="7" />
                                        <Label Content="N6" FontSize="18" FontWeight="Bold" HorizontalAlignment="Center" Margin="0,3,0,0" Name="labN6PDF" VerticalAlignment="Top" Grid.Column="8" />
                                        <Label Content="Ave" FontSize="18" FontWeight="Bold" HorizontalAlignment="Center" Margin="0,3,0,0" Name="labAvePDF" VerticalAlignment="Top" Grid.Column="9" />

                                        <!-- -->
                                        <!-- Maximun -->
                                        <Label Content="Maximun Force " FontSize="18" FontWeight="Bold" Margin="0,3,3,0" Name="labMaximunForce" VerticalAlignment="Top" Grid.Row="1" HorizontalAlignment="Right" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtMAXFORCE_WYarn" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="1" Grid.Column="1" Text="Warp" IsReadOnly="True"/>
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtMAXFORCE_WSpecification" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="1" Grid.Column="2" IsReadOnly="True"/>
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtMAXFORCE_WN1" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="1" Grid.Column="3" PreviewKeyDown="Common_PreviewKeyDown" KeyDown="txtMAXFORCE_WN1_KeyDown" LostFocus="MAXFORCE_W_LostFocus" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtMAXFORCE_WN2" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="1" Grid.Column="4" PreviewKeyDown="Common_PreviewKeyDown" KeyDown="txtMAXFORCE_WN2_KeyDown" LostFocus="MAXFORCE_W_LostFocus" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtMAXFORCE_WN3" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="1" Grid.Column="5" PreviewKeyDown="Common_PreviewKeyDown" KeyDown="txtMAXFORCE_WN3_KeyDown" LostFocus="MAXFORCE_W_LostFocus" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtMAXFORCE_WN4" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="1" Grid.Column="6" PreviewKeyDown="Common_PreviewKeyDown" KeyDown="txtMAXFORCE_WN4_KeyDown" LostFocus="MAXFORCE_W_LostFocus" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtMAXFORCE_WN5" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="1" Grid.Column="7" PreviewKeyDown="Common_PreviewKeyDown" KeyDown="txtMAXFORCE_WN5_KeyDown" LostFocus="MAXFORCE_W_LostFocus" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtMAXFORCE_WN6" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="1" Grid.Column="8" PreviewKeyDown="Common_PreviewKeyDown" KeyDown="txtMAXFORCE_WN6_KeyDown" LostFocus="MAXFORCE_W_LostFocus" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtMAXFORCE_WAve" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="1" Grid.Column="9" PreviewKeyDown="Common_PreviewKeyDown" IsReadOnly="True"/>

                                        <!-- MAXFORCE_F -->
                                        <Label Content="(Tensile) : " FontSize="18" FontWeight="Bold" Margin="0,3,3,0" Name="labMAXFORCE_F" VerticalAlignment="Top" Grid.Row="2" HorizontalAlignment="Right"/>
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtMAXFORCE_FYarn" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="2" Grid.Column="1" Text="Weft" IsReadOnly="True"/>
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtMAXFORCE_FSpecification" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="2" Grid.Column="2" IsReadOnly="True"/>
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtMAXFORCE_FN1" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="2" Grid.Column="3" PreviewKeyDown="Common_PreviewKeyDown" LostFocus="MAXFORCE_F_LostFocus" KeyDown="txtMAXFORCE_FN1_KeyDown" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtMAXFORCE_FN2" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="2" Grid.Column="4" PreviewKeyDown="Common_PreviewKeyDown" LostFocus="MAXFORCE_F_LostFocus" KeyDown="txtMAXFORCE_FN2_KeyDown" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtMAXFORCE_FN3" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="2" Grid.Column="5" PreviewKeyDown="Common_PreviewKeyDown" LostFocus="MAXFORCE_F_LostFocus" KeyDown="txtMAXFORCE_FN3_KeyDown" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtMAXFORCE_FN4" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="2" Grid.Column="6" PreviewKeyDown="Common_PreviewKeyDown" LostFocus="MAXFORCE_F_LostFocus" KeyDown="txtMAXFORCE_FN4_KeyDown" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtMAXFORCE_FN5" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="2" Grid.Column="7" PreviewKeyDown="Common_PreviewKeyDown" LostFocus="MAXFORCE_F_LostFocus" KeyDown="txtMAXFORCE_FN5_KeyDown" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtMAXFORCE_FN6" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="2" Grid.Column="8" PreviewKeyDown="Common_PreviewKeyDown" LostFocus="MAXFORCE_F_LostFocus" KeyDown="txtMAXFORCE_FN6_KeyDown" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtMAXFORCE_FAve" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="2" Grid.Column="9" PreviewKeyDown="Common_PreviewKeyDown" IsReadOnly="True"/>

                                        <!-- ELONGATIONFORCE_W -->
                                        <Label Content="Elongation at" FontSize="18" FontWeight="Bold" Margin="0,3,3,0" Name="labELONGATIONFORCE_W" VerticalAlignment="Top" Grid.Row="3" Grid.RowSpan="2" HorizontalAlignment="Right" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtELONGATIONFORCE_WYarn" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="3" Grid.Column="1" Text="Warp" IsReadOnly="True"/>
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtELONGATIONFORCE_WSpecification" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="3" Grid.Column="2" IsReadOnly="True" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtELONGATIONFORCE_WN1" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="3" Grid.Column="3" PreviewKeyDown="Common_PreviewKeyDown" LostFocus="ELONGATIONFORCE_W_LostFocus" KeyDown="txtELONGATIONFORCE_WN1_KeyDown" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtELONGATIONFORCE_WN2" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="3" Grid.Column="4" PreviewKeyDown="Common_PreviewKeyDown" LostFocus="ELONGATIONFORCE_W_LostFocus" KeyDown="txtELONGATIONFORCE_WN2_KeyDown" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtELONGATIONFORCE_WN3" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="3" Grid.Column="5" PreviewKeyDown="Common_PreviewKeyDown" LostFocus="ELONGATIONFORCE_W_LostFocus" KeyDown="txtELONGATIONFORCE_WN3_KeyDown" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtELONGATIONFORCE_WN4" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="3" Grid.Column="6" PreviewKeyDown="Common_PreviewKeyDown" LostFocus="ELONGATIONFORCE_W_LostFocus" KeyDown="txtELONGATIONFORCE_WN4_KeyDown" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtELONGATIONFORCE_WN5" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="3" Grid.Column="7" PreviewKeyDown="Common_PreviewKeyDown" LostFocus="ELONGATIONFORCE_W_LostFocus" KeyDown="txtELONGATIONFORCE_WN5_KeyDown" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtELONGATIONFORCE_WN6" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="3" Grid.Column="8" PreviewKeyDown="Common_PreviewKeyDown" LostFocus="ELONGATIONFORCE_W_LostFocus" KeyDown="txtELONGATIONFORCE_WN6_KeyDown" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtELONGATIONFORCE_WAve" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="3" Grid.Column="9" PreviewKeyDown="Common_PreviewKeyDown" IsReadOnly="True"/>

                                        <!-- ELONGATIONFORCE_F -->
                                        <Label Content="Maximum Force : " FontSize="18" FontWeight="Bold" Margin="0,3,3,0" Name="labELONGATIONFORCE_F" VerticalAlignment="Top" Grid.Row="4" HorizontalAlignment="Right"/>
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtELONGATIONFORCE_FYarn" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="4" Grid.Column="1" Text="Weft" IsReadOnly="True"/>
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtELONGATIONFORCE_FSpecification" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="4" Grid.Column="2" IsReadOnly="True"/>
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtELONGATIONFORCE_FN1" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="4" Grid.Column="3" PreviewKeyDown="Common_PreviewKeyDown" LostFocus="ELONGATIONFORCE_F_LostFocus" KeyDown="txtELONGATIONFORCE_FN1_KeyDown" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtELONGATIONFORCE_FN2" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="4" Grid.Column="4" PreviewKeyDown="Common_PreviewKeyDown" LostFocus="ELONGATIONFORCE_F_LostFocus" KeyDown="txtELONGATIONFORCE_FN2_KeyDown" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtELONGATIONFORCE_FN3" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="4" Grid.Column="5" PreviewKeyDown="Common_PreviewKeyDown" LostFocus="ELONGATIONFORCE_F_LostFocus" KeyDown="txtELONGATIONFORCE_FN3_KeyDown" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtELONGATIONFORCE_FN4" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="4" Grid.Column="6" PreviewKeyDown="Common_PreviewKeyDown" LostFocus="ELONGATIONFORCE_F_LostFocus" KeyDown="txtELONGATIONFORCE_FN4_KeyDown" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtELONGATIONFORCE_FN5" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="4" Grid.Column="7" PreviewKeyDown="Common_PreviewKeyDown" LostFocus="ELONGATIONFORCE_F_LostFocus" KeyDown="txtELONGATIONFORCE_FN5_KeyDown" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtELONGATIONFORCE_FN6" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="4" Grid.Column="8" PreviewKeyDown="Common_PreviewKeyDown" LostFocus="ELONGATIONFORCE_F_LostFocus" KeyDown="txtELONGATIONFORCE_FN6_KeyDown" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtELONGATIONFORCE_FAve" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="4" Grid.Column="9" PreviewKeyDown="Common_PreviewKeyDown" IsReadOnly="True"/>

                                        <!-- EDGECOMB_W -->
                                        <Label Content="Edgecomb" FontSize="18" FontWeight="Bold" Margin="0,3,3,0" Name="labEDGECOMB_W" VerticalAlignment="Top" Grid.Row="5" Grid.RowSpan="2" HorizontalAlignment="Right" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtEDGECOMB_WYarn" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="5" Grid.Column="1" Text="Warp" IsReadOnly="True"/>
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtEDGECOMB_WSpecification" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="5" Grid.Column="2" IsReadOnly="True" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtEDGECOMB_WN1" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="5" Grid.Column="3" PreviewKeyDown="Common_PreviewKeyDown" LostFocus="EDGECOMB_W_LostFocus" KeyDown="txtEDGECOMB_WN1_KeyDown" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtEDGECOMB_WN2" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="5" Grid.Column="4" PreviewKeyDown="Common_PreviewKeyDown" LostFocus="EDGECOMB_W_LostFocus" KeyDown="txtEDGECOMB_WN2_KeyDown" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtEDGECOMB_WN3" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="5" Grid.Column="5" PreviewKeyDown="Common_PreviewKeyDown" LostFocus="EDGECOMB_W_LostFocus" KeyDown="txtEDGECOMB_WN3_KeyDown" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtEDGECOMB_WN4" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="5" Grid.Column="6" PreviewKeyDown="Common_PreviewKeyDown" LostFocus="EDGECOMB_W_LostFocus" KeyDown="txtEDGECOMB_WN4_KeyDown" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtEDGECOMB_WN5" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="5" Grid.Column="7" PreviewKeyDown="Common_PreviewKeyDown" LostFocus="EDGECOMB_W_LostFocus" KeyDown="txtEDGECOMB_WN5_KeyDown" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtEDGECOMB_WN6" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="5" Grid.Column="8" PreviewKeyDown="Common_PreviewKeyDown" LostFocus="EDGECOMB_W_LostFocus" KeyDown="txtEDGECOMB_WN6_KeyDown" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtEDGECOMB_WAve" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="5" Grid.Column="9" PreviewKeyDown="Common_PreviewKeyDown" IsReadOnly="True"/>

                                        <!-- EDGECOMB_F -->
                                        <Label Content="Resistance : " FontSize="18" FontWeight="Bold" Margin="0,3,3,0" Name="labEDGECOMB_F" VerticalAlignment="Top" Grid.Row="6" HorizontalAlignment="Right"/>
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtEDGECOMB_FYarn" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="6" Grid.Column="1" Text="Weft" IsReadOnly="True"/>
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtEDGECOMB_FSpecification" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="6" Grid.Column="2" IsReadOnly="True"/>
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtEDGECOMB_FN1" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="6" Grid.Column="3" PreviewKeyDown="Common_PreviewKeyDown" LostFocus="EDGECOMB_F_LostFocus" KeyDown="txtEDGECOMB_FN1_KeyDown" />
                                        <TextBox FontSize="18" FontWeight="Bold" Margin="3,7,3,0" Name="txtEDGECOMB_FN2" TextAlignment="Center" VerticalAlignment="Top" Grid.Row="6" Grid.Column="4" PreviewKeyDown="Common_PreviewKeyDown" LostFocus="EDGECOMB_F_LostFocus" KeyDown="txtEDGECOMB_FN2_KeyDown" />
